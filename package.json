{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "Boilerplate for React JS & Express JS",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "cd client && npm install"
  },
  "author": "Alister Pereira",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.1.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  }
}
